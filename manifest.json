{
  "manifest_version": 3,
  "name": "Extension GPE",
  "version": "1.0",
  "permissions": ["storage", "http://server.gpeclub.com:3000/"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": "GPE.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [{
    "resources": ["GPE.png"],
    "matches": ["<all_urls>"]
  }]
}