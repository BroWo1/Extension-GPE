{
  "manifest_version": 3,
  "name": "Extension GPE",
  "version": "1.1",
  "permissions": ["storage", "https://server.gpeclub.com:1000/"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": "GPE.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["marked.min.js", "content.js"],
      "css": ["gpe-style.css"]
    }
  ],
  "web_accessible_resources": [{
    "resources": ["GPE.png"],
    "matches": ["<all_urls>"]
  }]
}